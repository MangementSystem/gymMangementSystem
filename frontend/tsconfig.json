{
    "compilerOptions": {
        "jsx": "react-jsx",
        "module": "esnext",
        "target": "esnext",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "allowJs": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "isolatedModules": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "incremental": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": [
                "*"
            ]
        },
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        ".next/dev/types/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}